{
  "name": "@typhonjs-fvtt/trl-documentation",
  "version": "2023.5.1",
  "description": "Provides comprehensive documentation for the TyphonJS Runtime Library (Foundry VTT edition).",
  "license": "MPL-2.0",
  "private": true,
  "type": "module",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "sideEffects": false,
  "imports": {
     "#runtime/*": "./.doc-gen/#runtime/*/index.d.ts",
     "#standard/*": "./.doc-gen/#standard/*/index.d.ts",
     "#svelte/internal": "svelte/internal",
     "#svelte/*": "./.doc-gen/svelte/*/index.d.ts",
     "#svelte": "./.doc-gen/svelte/index.d.ts"
  },
  "exports": {
    "./plugin/foundry-links": {
      "types": "./types/index-plugin.d.ts",
      "require": "./dist/plugin/foundry-links/index.cjs"
    }
  },
  "devDependencies": {
    "@types/node": "^20.3.3",
    "@typhonjs-build-test/node": "^0.0.9",
    "@typhonjs-fvtt/runtime": "^0.0.22",
    "@typhonjs-fvtt/svelte-standard": "^0.0.23",
    "@typhonjs-typedoc/typedoc-theme-dmt": "github:typhonjs-typedoc/typedoc-theme-dmt",
    "svelte": "^4.0.5",
    "svelte-preprocess" : "^5.0.4",
    "svelte-tag": "^1.0.2",
    "sirv": "^2",
    "polka": "^0.5.2",
    "typedoc": "^0.24.8",
    "typedoc-plugin-coverage": "^2.1.0",
    "typescript": "^5.1.6",
    "upath": "^2.0.1"
  },
  "browserslist": [">5%", "not IE 11"],
  "scripts": {
    "build-docs": "node src/generate/generate.js",
    "build-plugin": "rollup -c",
    "serve": "node src/serve/serve.js",
    "eslint": "eslint ."
  }
}
